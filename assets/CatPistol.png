<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" aria-labelledby="title" role="img">
  <title id="title">Flash-style cat sprite with high camo bandana and toy pistol</title>
  
  <!-- Cat -->
  <g fill="currentColor" stroke="currentColor" stroke-linejoin="round" stroke-linecap="round" stroke-width=".6">
    <!-- Tail (lowered) -->
    <path d="M12.5 18.5c1.6 0 2.8 1.1 2.8 2.4s-.8 2-1.8 2c-.5 0-.9-.2-.9-.6 0-.3.2-.5.5-.6.9-.3 1-1.6-.6-2.3" fill="currentColor"/>
    
    <!-- Body -->
    <ellipse cx="10.2" cy="14.8" rx="4.6" ry="5.4"/>
    <!-- White belly patch -->
    <ellipse cx="10.2" cy="15.6" rx="2" ry="2.6" fill="#fff" stroke="none" opacity=".9"/>
    <!-- Feet -->
    <ellipse cx="8.2" cy="19.6" rx="1" ry=".6"/>
    <ellipse cx="12.2" cy="19.6" rx="1" ry=".6"/>

    <!-- Head -->
    <circle cx="9.6" cy="7.8" r="3.8"/>
    <!-- Ears -->
    <path d="M7.4 5.2 8.5 3.2 9.5 5.1Z"/>
    <path d="M10.6 5.1 11.6 3.2 12.6 5.2Z"/>
    <!-- Inner ears -->
    <path d="M8.7 5.2 9.4 4 10 5.2Z" fill="currentColor" opacity=".22" stroke="none"/>
    <path d="M10.3 5.2 11 4 11.6 5.2Z" fill="currentColor" opacity=".22" stroke="none"/>

    <!-- Eyes -->
    <g stroke="none">
      <ellipse cx="8.7" cy="7.7" rx=".55" ry=".7" fill="#fff"/>
      <ellipse cx="10.6" cy="7.7" rx=".55" ry=".7" fill="#fff"/>
      <circle cx="8.8" cy="7.8" r=".25" fill="currentColor"/>
      <circle cx="10.7" cy="7.8" r=".25" fill="currentColor"/>
      <circle cx="8.7" cy="7.6" r=".1" fill="#fff"/>
      <circle cx="10.6" cy="7.6" r=".1" fill="#fff"/>
    </g>

    <!-- Muzzle + nose -->
    <ellipse cx="9.6" cy="9.1" rx="1.5" ry="1.1" fill="#fff" stroke="none" opacity=".9"/>
    <path d="M9.3 8.7h.6l-.3.35Z" fill="currentColor" stroke="none"/>
    <!-- Whiskers -->
    <path d="M7.2 9.2h1.5M10.6 9.2h1.5M7.3 9.6h1.4M10.7 9.6h1.4" stroke="currentColor" stroke-width=".4" fill="none"/>

    <!-- Arm / paw -->
    <path d="M11.3 11.3c1.1-.5 2.2-.9 3.1-1l.4 1.2c-.8.2-1.7.6-2.7 1.2Z"/>
    <circle cx="15" cy="11.2" r=".65"/>
  </g>

  <!-- Camo bandana (raised up just under ears) -->
  <g stroke="none">
    <!-- base strip -->
    <path d="M6.0 5.8c1.9-.8 5.3-.8 7.2 0l-1.3 1.1c-1.6-.3-2.9-.3-4.6 0Z" fill="#4b6e2f"/>
    <!-- camo blotches -->
    <circle cx="8.1" cy="6.1" r=".35" fill="#2f4420"/>
    <circle cx="10.4" cy="6" r=".35" fill="#6c8c40"/>
    <circle cx="9.2" cy="6.4" r=".3" fill="#2f4420"/>
  </g>

  <!-- Toy pistol -->
  <g fill="#8a8f98" stroke="#43464a" stroke-width=".6" stroke-linejoin="round" stroke-linecap="round">
    <rect x="15.1" y="9.2" width="3.4" height="1.4" rx=".3"/>
    <rect x="18.5" y="9.35" width="1.1" height="1.1" rx=".3" fill="#ff7a2f" stroke="#c65119"/>
    <path d="M16.3 10.6l.9 1.1h-.9z"/>
    <path d="M16.6 10.8c.4 0 .6.2.6.4 0 .2-.2.3-.6.3" fill="none"/>
  </g>
</svg>
